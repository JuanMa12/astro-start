---
export interface Props {
  title: string;
  title2: string;
  subtitle: string;
}

const { title, title2, subtitle } = Astro.props;
---

<div class="text-green-400 text-4xl font-bold text-center">
  {title}
  <br />
  {title2}
</div>
<h2 class="text-lg text-center">{subtitle}</h2>
<hr class="solid" />
